{
  "presets": [
    "react",    
    "stage-2",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": [
    "babel-plugin-module-resolver",
    "dva-hmr",
    "transform-class-properties",
    "transform-runtime",
    "transform-decorators-legacy"
  ],

}